<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- App Sandbox: Enable sandboxing for security -->
	<key>com.apple.security.app-sandbox</key>
	<true/>
	
	<!-- Network: Allow outgoing network connections for IPTV streaming -->
	<key>com.apple.security.network.client</key>
	<true/>
	
	<!-- File Access: Allow reading/writing user-selected files (M3U imports) -->
	<key>com.apple.security.files.user-selected.read-write</key>
	<true/>
	
	<!-- Hardened Runtime: Disable JIT compilation for security -->
	<key>com.apple.security.cs.allow-jit</key>
	<false/>
	
	<!-- Hardened Runtime: Disable unsigned executable memory -->
	<key>com.apple.security.cs.allow-unsigned-executable-memory</key>
	<false/>
	
	<!-- Hardened Runtime: Disable DYLD environment variables -->
	<key>com.apple.security.cs.allow-dyld-environment-variables</key>
	<false/>
	
	<!-- Hardened Runtime: Enable library validation -->
	<key>com.apple.security.cs.disable-library-validation</key>
	<false/>
</dict>
</plist>
